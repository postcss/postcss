{
  "source": {
    "includePattern": ".+\\.es6$"
  },
  "opts": {
    "template": "node_modules/docdash",
    "destination": "./api/"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "default": {
      "layoutFile": "node_modules/docdash/tmpl/layout.tmpl",
      "includeDate": false
    }
  }
}
